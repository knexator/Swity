data Tree {
    ("Literal" Literal),
    ("Variable" Literal), // TODO: a peano number?
    ("Plex" ListOfTrees),
    // ("Comptime" Tree Tree) // probably remove
}

data ListOfTrees {
    "nil",
    (Tree ListOfTrees),
}

data Case {
    (Tree Tree Tree {
        "return",
        ListOfCases,
    }),
}

data ListOfCases {
    "nil",
    (Case ListOfCases),
}

data Literal @AnyLiteral