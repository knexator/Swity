include "../stdlib/meta.swt";

data Foo {
    "a",
    "b",
}

fn constantFunction: @Any -> ListOfCases {
    v -> @toTree: v {
        v2 -> ((("Variable" "x") ("Literal" "@identity") v2 "return") "nil");
    }
}

fn myFunc: Foo -> Foo {
    v -> (constantFunction "b"): v;
}

main myFunc: "a";
