include "../stdlib/meta.swt";

data Foo {
    "a",
    "b",
}

fn constantFunction: Foo -> ListOfCases {
    // TODO: not actually ("Literal" v), should be <toTree v> maybe
    v -> ((("Variable" "x") "identity" ("Literal" v) "return") "nil");
}

fn myFunc: Foo -> Foo {
    v -> (constantFunction "b"): v;
}

main myFunc: "a";
